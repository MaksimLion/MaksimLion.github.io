(function($){$.fn.animated=function(inEffect,outEffect,event){$(this).css("opacity","0").addClass("animated").waypoint(function(dir){if(dir==="down"){$(this.element).removeClass(outEffect).addClass(inEffect).css("opacity","1");}else{$(this.element).removeClass(inEffect).addClass(outEffect).css("opacity","1");}if(event){event($(this.element))}},{offset:"80%"});};})(jQuery);